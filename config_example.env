# V-Agent 环境配置示例
# 复制此文件为 .env 并修改相应值

# LLM配置
CAF_LLM_PROVIDER=dashscope
CAF_LLM_MODEL=qwen-plus
CAF_LLM_TEMPERATURE=0.7
CAF_LLM_MAX_TOKENS=4096

# 协调器配置 - 提升最大迭代次数
CAF_MAX_ITERATIONS=20
CAF_QUALITY_THRESHOLD=0.7

# 智能体配置
CAF_AGENT_TIMEOUT=120.0
CAF_ENABLE_CACHE=true

# 日志配置
CAF_LOG_LEVEL=INFO
CAF_OUTPUT_DIR=./output
CAF_ENABLE_DETAILED_LOGGING=true

# 新增：高级迭代配置
CAF_MAX_SELF_ITERATIONS=5
CAF_MAX_COORDINATION_ITERATIONS=15
CAF_MAX_AGENT_ITERATIONS=25 