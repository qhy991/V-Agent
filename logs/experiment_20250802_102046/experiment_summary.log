    // 其他位加法
    genvar i;
    generate
        for (i = 1; i < 8; i = i + 1) begin : fa_block
            full_adder fa (
                .a(a[i]),
                .b(b[i]),
                .cin(carry[i-1]),
                .sum(sum[i]),
                .cout(carry[i])
            );
        end
    endgenerate

    // 输出进位
    assign cout = carry[7];

endmodule

// 全加器模块
module full_adder (
    input  a,
    input  b,
    input  cin,
    output sum,
    output cout
);

    wire xor1, xor2;
    wire and1, and2, and3;

    // XOR逻辑
    xor (xor1, a, b);
    xor (xor2, xor1, cin);
    assign sum = xor2;

    // AND逻辑
    and (and1, a, b);
    and (and2, a, cin);
    and (and3, b, cin);

    // OR逻辑
    or (cout, and1, and2, and3);

endmodule
