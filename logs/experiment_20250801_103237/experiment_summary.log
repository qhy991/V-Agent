parameter WIDTH = 8;

// 内部信号声明
wire [WIDTH-1:0] carry;

// 逐位加法逻辑
genvar i;
generate
    for (i = 0; i < WIDTH; i = i + 1) begin : adder_stage
        // 第0位处理进位输入
        if (i == 0) begin
            full_adder fa (
                .a(a[i]),
                .b(b[i]),
                .cin(cin),
                .sum(sum[i]),
                .cout(carry[i])
            );
        end else begin
            full_adder fa (
                .a(a[i]),
                .b(b[i]),
                .cin(carry[i-1]),
                .sum(sum[i]),
                .cout(carry[i])
            );
        end
    end
endgenerate

// 输出进位连接到最高位的进位输出
assign cout = carry[WIDTH-1];

// 全加器模块定义
module full_adder (
    input  a,
    input  b,
    input  cin,
    output sum,
    output cout
);
    // 计算和
    assign sum = a ^ b ^ cin;
    
    // 计算进位
    assign cout = (a & b) | (a & cin) | (b & cin);
endmodule

endmodule
