        cin = 1'b0;
        #CLK_PERIOD;
        assert (sum == 16'h0000 && cout == 1'b1 && overflow == 1'b0)
            $display("Carry propagation test passed.");
        else
            $display("Carry propagation test failed: sum=%04h, cout=%b, overflow=%b", sum, cout, overflow);
    endtask

    // 溢出检测测试
    task overflow_test;
        $display("Running overflow_test...");

        // 正数 + 正数 → 负数（溢出）
        a = 16'h7FFF;
        b = 16'h0001;
        cin = 1'b0;
        #CLK_PERIOD;
        assert (sum == 16'h8000 && cout == 1'b0 && overflow == 1'b1)
            $display("Overflow test 1 passed.");
        else
            $display("Overflow test 1 failed: sum=%04h, cout=%b, overflow=%b", sum, cout, overflow);

        // 负数 + 负数 → 正数（溢出）
        a = 16'h8000;
        b = 16'h8000;
        cin = 1'b0;
        #CLK_PERIOD;
        assert (sum == 16'h0000 && cout == 1'b1 && overflow == 1'b1)
            $display("Overflow test 2 passed.");
        else
            $display("Overflow test 2 failed: sum=%04h, cout=%b, overflow=%b", sum, cout, overflow);
    endtask

    // 边界值测试
    task boundary_value_test;
        $display("Running boundary_value_test...");

        // 0x0000 + 0x0000
        a = 16'h0000;
        b = 16'h0000;
        cin = 1'b0;
        #CLK_PERIOD;
        assert (sum == 16'h0000 && cout == 1'b0 && overflow == 1'b0)
            $display("Boundary test 1 passed.");
        else
            $display("Boundary test 1 failed: sum=%04h, cout=%b, overflow=%b", sum, cout, overflow);

        // 0xFFFF + 0x0001
        a = 16'hFFFF;
        b = 16'h0001;
        cin = 1'b0;
        #CLK_PERIOD;
        assert (sum == 16'h0000 && cout == 1'b1 && overflow == 1'b0)
            $display("Boundary test 2 passed.");
        else
            $display("Boundary test 2 failed: sum=%04h, cout=%b, overflow=%b", sum, cout, overflow);
    endtask

    // 随机数据测试
    task random_data_test;
        $display("Running random_data_test...");

        for (int i = 0; i < 100; i++) begin
            a = $random;
            b = $random;
            cin = $random % 2;
            #CLK_PERIOD;

            // 简单验证：检查进位是否正确
            if (a + b + cin > 16'hFFFF) begin
                assert (cout == 1'b1)
                    $display("Random test %d passed.", i);
                else
                    $display("Random test %d failed: expected cout=1, got %b", i, cout);
            end else begin
                assert (cout == 1'b0)
                    $display("Random test %d passed.", i);
                else
                    $display("Random test %d failed: expected cout=0, got %b", i, cout);
            end
        end
    endtask

endmodule
```

