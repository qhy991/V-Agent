module full_adder (
    input  a,
    input  b,
    input  cin,
    output sum,
    output cout
);

    wire xor1, xor2;
    wire and1, and2, and3;

    // XOR for sum
    assign xor1 = a ^ b;
    assign xor2 = xor1 ^ cin;
    assign sum = xor2;

    // AND for carry out
    assign and1 = a & b;
    assign and2 = a & cin;
    assign and3 = b & cin;
    assign cout = (and1 | and2) | and3;

endmodule
