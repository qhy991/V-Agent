    %store/vec4 v000001af53cc1610_0, 0, 4;
    %delay 10000, 0;
    %load/vec4 v000001af53cc14d0_0;
    %load/vec4 v000001af53c5e000_0;
    %load/vec4 v000001af53c6c5a0_0;
    %add;
    %cmp/ne;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v000001af53c5e000_0;
    %load/vec4 v000001af53c6c5a0_0;
    %add;
    %vpi_call 2 102 "$display", "Addition Test Failed at cycle %0d: Expected 0x%08h, Got 0x%08h", v000001af53bf3510_0, S<0,vec4,u32>, v000001af53cc14d0_0 {1 0 0};
