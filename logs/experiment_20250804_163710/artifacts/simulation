    %load/vec4 v000001af53bf3510_0;
    %cmpi/s 10, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v000001af53bf3510_0;
    %muli 100, 0, 32;
    %store/vec4 v000001af53c5e000_0, 0, 32;
    %load/vec4 v000001af53bf3510_0;
    %muli 50, 0, 32;
    %store/vec4 v000001af53c6c5a0_0, 0, 32;
