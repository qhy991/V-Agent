        expected_sum = a_val + b_val + cin_val;
        expected_cout = (expected_sum > 8'hFF) ? 1'b1 : 1'b0;

        // 显示预期与实际结果
        $display("Test case: a = %b, b = %b, cin = %b | Expected sum = %b, Expected cout = %b | Actual sum = %b, Actual cout = %b",
