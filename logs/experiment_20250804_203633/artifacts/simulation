:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x14a7063e0 .scope module, "counter_4bit_tb" "counter_4bit_tb" 2 3;
 .timescale -9 -12;
v0x14a7190e0_0 .var "clk", 0 0;
v0x14a719180_0 .net "count", 3 0, v0x14a718e80_0;  1 drivers
v0x14a719230_0 .var "enable", 0 0;
v0x14a719300_0 .var "reset_n", 0 0;
S_0x14a706550 .scope module, "uut" "counter_4bit" 2 14, 3 1 0, S_0x14a7063e0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset_n";
    .port_info 2 /INPUT 1 "enable";
    .port_info 3 /OUTPUT 4 "count";
v0x14a704990_0 .net "clk", 0 0, v0x14a7190e0_0;  1 drivers
v0x14a718e80_0 .var "count", 3 0;
v0x14a718f30_0 .net "enable", 0 0, v0x14a719230_0;  1 drivers
v0x14a718fe0_0 .net "reset_n", 0 0, v0x14a719300_0;  1 drivers
E_0x14a706db0/0 .event negedge, v0x14a718fe0_0;
E_0x14a706db0/1 .event posedge, v0x14a704990_0;
E_0x14a706db0 .event/or E_0x14a706db0/0, E_0x14a706db0/1;
    .scope S_0x14a706550;
T_0 ;
    %wait E_0x14a706db0;
    %load/vec4 v0x14a718fe0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x14a718e80_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x14a718f30_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x14a718e80_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x14a718e80_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x14a7063e0;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x14a7190e0_0;
    %inv;
    %store/vec4 v0x14a7190e0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x14a7063e0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14a7190e0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14a719300_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14a719230_0, 0, 1;
    %vpi_call 2 34 "$display", "Time\011clk\011reset_n\011enable\011count" {0 0 0};
    %vpi_call 2 35 "$monitor", "%0t\011%b\011%b\011%b\011%0d", $time, v0x14a7190e0_0, v0x14a719300_0, v0x14a719230_0, v0x14a719180_0 {0 0 0};
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x14a719300_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14a719300_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x14a719300_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x14a719230_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14a719230_0, 0, 1;
    %delay 50000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x14a719230_0, 0, 1;
    %delay 400000, 0;
    %vpi_call 2 56 "$finish" {0 0 0};
    %end;
    .thread T_2;
