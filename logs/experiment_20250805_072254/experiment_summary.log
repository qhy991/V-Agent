module four_bit_adder (
    input [3:0] a,
    input [3:0] b,
    output [3:0] sum,
    output cout
);

    // Internal signal for carry propagation
    wire [3:0] carry;

    // Assign the least significant carry-in to 0
    assign carry[0] = 1'b0;

    // Full adder logic for each bit position
    assign sum[0] = a[0] ^ b[0] ^ carry[0];
    assign carry[1] = (a[0] & b[0]) | (carry[0] & (a[0] ^ b[0]));

