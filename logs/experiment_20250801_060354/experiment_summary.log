// Calculate sum bits
generate
    for (i = 0; i < DATA_WIDTH; i = i + 1) begin : gen_sum
        assign sum[i] = propagate_signal[i] ^ carry[i];
    end
endgenerate

// Output the final carry
assign cout = carry[DATA_WIDTH];

endmodule
