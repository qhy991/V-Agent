`timescale 1ns / 1ps

module counter_tb;

    // Parameters
    localparam WIDTH = 4;
    localparam CLK_PERIOD = 10;  // 10 ns clock period (100 MHz)

    // DUT signals
    reg         clk;
    reg         rst_n;
    reg         en;
    wire [WIDTH-1:0] count;

    // Instantiate Device Under Test
    counter #(.WIDTH(WIDTH)) dut (
        .clk(clk),
        .rst_n(rst_n),
        .en(en),
        .count(count)
    );

    // Clock generation
    always begin
        clk = 0;
        #(CLK_PERIOD / 2);
        clk = 1;
        #(CLK_PERIOD / 2);
    end

