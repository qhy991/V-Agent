    always @(posedge clk) begin
        if (!rst_n) begin
            count <= {WIDTH{1'b0}};  // Reset to zero
        end else if (en) begin
            count <= count + 1'b1;  // Increment on enable
        end
        // else: hold value
    end

    // Assertion: WIDTH must be positive
    // synthesis translate_off
    initial begin
        if (WIDTH < 1) begin
