// Date: 2025-08-05

`timescale 1ns / 1ps

module counter_tb;

    // Parameters
    localparam WIDTH = 4;
    localparam CLK_PERIOD = 10;  // 10 ns clock period (100 MHz)

    // DUT signals
    reg         clk;
    reg         rst_n;
    reg         en;
    wire [WIDTH-1:0] count;

    // Instantiate Device Under Test
    counter #(.WIDTH(WIDTH)) dut (
        .clk(clk),
        .rst_n(rst_n),
        .en(en),
        .count(count)
    );

    // Clock generation
    always begin
        clk = 0;
        #(CLK_PERIOD / 2);
        clk = 1;
        #(CLK_PERIOD / 2);
    end

    // Test sequence
    initial begin
        // Open dump file for waveform viewing (optional)
        $dumpfile("counter_tb.vcd");
        $dumpvars(0, counter_tb);

        // Initial values
        en    = 0;
        rst_n = 0;

        $display("[INFO] Starting counter testbench...");
        #(CLK_PERIOD * 2);

        // Release reset
        rst_n = 1;
        $display("[TEST] Reset released.");
        #(CLK_PERIOD * 2);

        // Enable counting
        en = 1;
        $display("[TEST] Enable activated. Counting...");
        repeat (10) @(posedge clk);

        // Disable counting
        en = 0;
        $display("[TEST] Enable deactivated. Holding value...");
        repeat (3) @(posedge clk);

        // Re-enable
        en = 1;
        $display("[TEST] Re-enable. Resuming count...");
        repeat (5) @(posedge clk);

        // Finish
        $display("[RESULT] Final count value: %d", count);
        $display("[INFO] Simulation completed.");
        $finish();
    end

endmodule
