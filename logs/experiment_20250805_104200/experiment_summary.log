`timescale 1ns / 1ps

module counter #(
    parameter WIDTH = 4  // Counter bit width (default 4-bit)
)(
    input               clk,        // Clock input (rising edge)
    input               rst_n,      // Active-low synchronous reset
    input               en,         // Enable signal (high to count)
