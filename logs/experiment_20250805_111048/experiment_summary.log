module counter_tb;

    // Parameters
    parameter int WIDTH = 4;
    parameter CLK_PERIOD = 10;

    // DUT signals
    reg         clk;
    reg         rst_n;
    reg         en;
    wire [WIDTH-1:0] count;
    wire             overflow;

    // Instantiate Device Under Test
    counter #(.WIDTH(WIDTH)) dut (
        .clk     (clk),
        .rst_n   (rst_n),
        .en      (en),
        .count   (count),
        .overflow(overflow)
    );

    // Clock generation
    always begin
        clk = 0;
        #(CLK_PERIOD / 2);
        clk = 1;
        #(CLK_PERIOD / 2);
    end

