        // Test 1: Enable counting
        en = 1;
        repeat (20) @(posedge clk);

        // Test 2: Disable during counting
        en = 0;
        repeat (5) @(posedge clk);

        // Re-enable
        en = 1;
        repeat (20) @(posedge clk);

        // Finish simulation
        $display("Simulation finished.");
