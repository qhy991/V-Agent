module counter_tb;

    // Parameters
    parameter int WIDTH = 4;
    parameter CLK_PERIOD = 10;

    // DUT signals
    reg         clk;
    reg         rst_n;
    reg         en;
    wire [WIDTH-1:0] count;
    wire             overflow;

    // Instantiate Device Under Test
    counter #(.WIDTH(WIDTH)) dut (
        .clk     (clk),
        .rst_n   (rst_n),
        .en      (en),
        .count   (count),
        .overflow(overflow)
    );

    // Clock generation
    always begin
        clk = 0;
        #(CLK_PERIOD / 2);
        clk = 1;
        #(CLK_PERIOD / 2);
    end

    // Test sequence
    initial begin
        $monitor("[%0t] rst_n=%b en=%b count=%d overflow=%b",
                 $time, rst_n, en, count, overflow);

        // Initialize inputs
        en    = 0;
        rst_n = 0;

        // Reset assertion
        @(posedge clk);
        @(posedge clk);

        rst_n = 1;  // Release reset
        @(posedge clk);

        // Test 1: Enable counting
        en = 1;
        repeat (20) @(posedge clk);

        // Test 2: Disable during counting
        en = 0;
        repeat (5) @(posedge clk);

        // Re-enable
        en = 1;
        repeat (20) @(posedge clk);

        // Finish simulation
        $display("Simulation finished.");
        $finish;
    end

endmodule
