    for (i = 1; i < WIDTH; i = i + 1) begin : gen_carry
        assign carry[i] = (a[i] & b[i]) | (a[i] & carry[i-1]) | (b[i] & carry[i-1]);
    end
endgenerate

// 计算每一位的和（异或运算）
assign sum = a ^ b ^ {carry[WIDTH-2], cin};

// 输出进位：最高位的进位
assign cout = carry[WIDTH-1];

